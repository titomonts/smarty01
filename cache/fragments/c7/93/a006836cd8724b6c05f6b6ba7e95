$(document).ready(function(){$.datepicker.setDefaults({dateFormat:"d MM yy",changeMonth:true,changeYear:true});$("#txtPreArriveDate").datepicker({dateFormat:"d MM yy",changeYear:true,minDate:0,onSelect:function(dateText,inst){$('#txtPreDepartDate').val("");var actualDate=new Date(dateText);var newDate=new Date(actualDate.getFullYear(),actualDate.getMonth(),actualDate.getDate()+1);$('#txtPreDepartDate').datepicker('option','minDate',newDate,'dateFormat','d MM yy');$('#txtPreDepartDate').datepicker({minDate:newDate,dateFormat:'d MM yy'});}});});